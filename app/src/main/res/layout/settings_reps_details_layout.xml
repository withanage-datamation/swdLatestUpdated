<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:background="@drawable/custom_label"
        android:layout_marginTop="10dp">

        <com.datamation.swdsfa.utils.CustomFont
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:text="User Name "
            android:textAllCaps="true"
            android:textColor="@color/unselected"
            android:textSize="@dimen/font_for_all"
            />
        <com.datamation.swdsfa.utils.CustomFont
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="left"
            android:text=":-"
            android:textSize="@dimen/font_for_all"
            android:visibility="visible" />

        <EditText
            android:id="@+id/et_rep_username"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:clickable="false"
            android:cursorVisible="false"
            android:ems="10"
            android:editable="false"
            android:textColor="@color/unselected"
            android:focusable="false"
            android:focusableInTouchMode="false">
        </EditText>

    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/custom_label">

        <com.datamation.swdsfa.utils.CustomFont
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:text="Rep Code"
            android:textAllCaps="true"
            android:textColor="@color/unselected"
            android:textSize="@dimen/font_for_all" />

        <com.datamation.swdsfa.utils.CustomFont
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="left"
            android:text=":-"
            android:textSize="@dimen/font_for_all"
            android:visibility="visible" />

        <EditText
            android:id="@+id/et_rep_code"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:clickable="false"
            android:cursorVisible="false"
            android:ems="10"
            android:editable="false"
            android:textColor="@color/unselected"
            android:focusable="false"
            android:focusableInTouchMode="false" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:background="@drawable/custom_label"
        android:layout_marginTop="10dp">

        <com.datamation.swdsfa.utils.CustomFont
            android:id="@+id/TextView03"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:text="Prefix"
            android:textAllCaps="true"
            android:textColor="@color/unselected"
            android:textSize="@dimen/font_for_all" />

        <com.datamation.swdsfa.utils.CustomFont
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="left"
            android:text=":-"
            android:textSize="@dimen/font_for_all"
            android:visibility="visible" />

        <EditText
            android:id="@+id/et_rep_prefix"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:textColor="@color/unselected"
            android:clickable="false"
            android:cursorVisible="false"
            android:ems="10"
            android:editable="false"
            android:focusable="false"
            android:focusableInTouchMode="false" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:background="@drawable/custom_label"
        android:layout_marginTop="10dp">

        <com.datamation.swdsfa.utils.CustomFont
            android:id="@+id/TextView04"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:text="Loc Code"
            android:textAllCaps="true"
            android:textColor="@color/unselected"
            android:textSize="@dimen/font_for_all"/>

        <com.datamation.swdsfa.utils.CustomFont
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="left"
            android:text=":-"
            android:textSize="@dimen/font_for_all"
            android:visibility="visible" />

        <EditText
            android:id="@+id/et_rep_loc_code"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:clickable="false"
            android:cursorVisible="false"
            android:ems="10"
            android:editable="false"
            android:focusable="false"
            android:textColor="@color/unselected"
            android:focusableInTouchMode="false" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:background="@drawable/custom_label"
        android:layout_marginTop="10dp">

        <com.datamation.swdsfa.utils.CustomFont
            android:id="@+id/TextView01"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:text="Area Code"
            android:textAllCaps="true"
            android:textColor="@color/unselected"
            android:textSize="@dimen/font_for_all" />

        <com.datamation.swdsfa.utils.CustomFont
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="left"
            android:text=":-"
            android:textSize="@dimen/font_for_all"
            android:visibility="visible" />

        <EditText
            android:id="@+id/et_rep_area_code"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:clickable="false"
            android:cursorVisible="false"
            android:ems="10"
            android:editable="false"
            android:focusable="false"
            android:textColor="@color/unselected"
            android:focusableInTouchMode="false" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="30dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:background="@drawable/custom_label"
        android:layout_marginTop="10dp">

        <com.datamation.swdsfa.utils.CustomFont
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:text="Deal Code"
            android:textAllCaps="true"
            android:textColor="@color/unselected"
            android:textSize="@dimen/font_for_all" />

        <com.datamation.swdsfa.utils.CustomFont
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="left"
            android:text=":-"
            android:textSize="@dimen/font_for_all"
            android:visibility="visible" />

        <EditText
            android:id="@+id/et_rep_deal_code"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:clickable="false"
            android:cursorVisible="false"
            android:ems="10"
            android:editable="false"
            android:focusable="false"
            android:textColor="@color/unselected"
            android:focusableInTouchMode="false" />
    </LinearLayout>

</LinearLayout>